{
    "name": "turnstile/turnstile",
    "description": "A Laravel package to integrate Cloudflare Turnstile",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Turnstile\\Turnstile\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Awais"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turnstile\\Turnstile\\Providers\\TurnstileServiceProvider"
            ],
            "aliases": {
                "Turnstile": "Turnstile\\Turnstile\\Facades\\Turnstile"
            }
        }
    },
    "prefer-stable": true

}